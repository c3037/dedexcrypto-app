{
    "name": "@dedexcrypto/app",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint 'src/**/*.+(ts|tsx|js|jsx)'",
        "format": "prettier 'src/**/*.+(ts|tsx|js|jsx)' --write",
